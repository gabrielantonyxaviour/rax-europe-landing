# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Resend Email Configuration
RESEND_API_KEY=your_resend_api_key

# PostHog Analytics
# Set NEXT_PUBLIC_POSTHOG_ENABLED to "true" to enable PostHog (requires key to be set)
NEXT_PUBLIC_POSTHOG_ENABLED=false
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Feature Toggles
# Set to "true" to show these sections, any other value (or unset) to hide them
SHOW_TESTIMONIALS_SECTION=false
SHOW_JOURNEY_SECTION=false
